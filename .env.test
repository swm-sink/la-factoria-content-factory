# Test environment configuration for La Factoria
ENVIRONMENT=test
DEBUG=true

# Disable Redis for testing to speed up tests
REDIS_URL=

# Test API key
LA_FACTORIA_API_KEY=test-api-key

# Test rate limits (more permissive for testing)
RATE_LIMIT_REQUESTS_PER_MINUTE=1000
RATE_LIMIT_GENERATIONS_PER_HOUR=100

# Disable AI providers for testing
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
ELEVENLABS_API_KEY=

# Test database
DEV_DATABASE_URL=sqlite:///./test_la_factoria.db